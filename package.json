{
  "name": "questioner",
  "version": "1.0.0",
  "description": "Questioner helps the meetup organizer prioritize questions to be answered. Other users can vote on asked questions and they bubble to the top or bottom of the log.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./api -s",
    "build": "npm run clean && npm run build-server",
    "start": "npm run build && node ./build/index.js",
    "start-dev": "node_modules/.bin/nodemon api/index.js --exec babel-node --",
    "lint": "eslint",
    "test": "nyc --reporter=text --cache mocha ./api/tests/**/*.js --require babel-core/register --exit && npm run coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oladims/questioner.git"
  },
  "author": "Olufowora Ibrahim Oladimeji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Oladims/questioner/issues"
  },
  "homepage": "https://github.com/Oladims/questioner#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "joi": "^14.3.1",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0"
  }
}
